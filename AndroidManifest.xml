<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.shaun.securityapp">
          <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
          <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".Entry"></activity>
        <activity android:name=".EntryView"></activity>
        <activity android:name=".Entries"></activity>
        <activity android:name=".SignUp"></activity>
        <activity android:name=".Generation"></activity>
        <activity android:name=".Checker"></activity>
        <activity android:name=".MainActivityZ"></activity>
       


    </application>

</manifest>
